{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend dev:post-start",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "dev:post-start": "sleep 10 && npm run sync:trending",
    "build": "tsc && vite build",
    "build:check": "tsc -p convex --noEmit --pretty false && tsc -p . --noEmit --pretty false",
    "lint": "npm run build:check",
    "preview": "vite preview",
    "check:env": "node scripts/check-env.cjs",
    "post-deploy": "sh scripts/post-deploy.sh",
    "sync:trending": "npx convex run internal.admin:refreshTrendingCacheInternal && npx convex run maintenance:triggerTrendingSync",
    "seed:setlists": "npx convex run maintenance:backfillMissingSetlists '{\"limit\":100}'",
    "import:setlists": "npx convex run setlistfm:triggerCompletedShowsCheck",
    "import:past-setlists": "echo 'Checking for completed shows and importing from setlist.fm...' && npx convex run setlistfm:triggerCompletedShowsCheck && echo 'Also refreshing trending caches...' && npm run sync:trending",
    "deploy:backend": "npx convex deploy --yes",
    "deploy:frontend": "vercel --prod --yes",
    "deploy": "npm run deploy:backend && npm run deploy:frontend",
    "all:fast": "npm run deploy && npm run post-deploy",
    "all": "npm run check:env && npm run lint && npm run test:run && npm run build && npm run all:fast",
    "all:full": "npm run all && npm run seed:setlists",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --passWithNoTests",
    "sanity:dev": "sanity dev",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.45.0",
    "@clerk/nextjs": "^6.31.6",
    "@convex-dev/auth": "^0.0.80",
    "@playwright/test": "^1.55.0",
    "@portabletext/react": "^5.0.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sanity/client": "^7.13.1",
    "@sanity/code-input": "^6.0.3",
    "@sanity/image-url": "^2.0.2",
    "@sanity/vision": "^4.20.3",
    "@types/cheerio": "^0.22.35",
    "@types/react-router-dom": "^5.3.3",
    "@vitest/ui": "^2.1.3",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.29.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.22",
    "jsdom": "^25.0.1",
    "lucide-react": "^0.542.0",
    "motion": "^12.23.12",
    "next-themes": "^0.4.6",
    "openai": "^5.16.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "react-window": "^1.8.10",
    "resend": "^6.5.2",
    "sanity": "^4.20.3",
    "sonner": "^2.0.7",
    "styled-components": "^6.1.15",
    "svix": "^1.76.1",
    "tailwind-merge": "^3.1.0",
    "tailwindcss-animate": "^1.0.7",
    "vitest": "^2.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.18.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "~10",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "~8",
    "prettier": "^3.5.3",
    "tailwindcss": "~3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
